{
  "preset": "ts-jest",
  "globals": {
    "ts-jest": {
      "diagnostics": false
    }
  },
  "roots": [
    "<rootDir>/src"
  ],
  "testEnvironment": "node",
  "testMatch": ["**/*.spec.ts"],
  "reporters": [
    "default",
    [
      "jest-junit",
      {
        "output": ".reports/junit/js-test-results.xml"
      }
    ]
  ]
}
