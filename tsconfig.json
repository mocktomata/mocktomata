{
  "extends": "./tsconfig.es2015.json",
  "atom": {
    "rewriteTsconfig": false
  },
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
    "inlineSourceMap": false,
    "sourceMap": true,
    "plugins": [
      {
        "name": "tslint-language-service"
      }
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "custom-typings",
    "src2"
  ],
  "files": [
    "src2/index"
  ]
}
