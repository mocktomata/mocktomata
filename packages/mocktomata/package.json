{
  "name": "mocktomata",
  "version": "7.0.0-beta.10",
  "description": "A Behavior Tracking and Simulation System",
  "keywords": [
    "boundary",
    "boundary-testing",
    "fixture",
    "testing",
    "testing-framework",
    "testing-tools"
  ],
  "homepage": "https://github.com/mocktomata/mocktomata",
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm",
    "dist"
  ],
  "browser": {},
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "mtmt": "lib/bin.js"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:libm && yarn build:browser",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:libm": "tsc -p tsconfig.libm.json",
    "build:browser": "webpack",
    "clean": "rimraf lib libm dist .tsc",
    "coverage": "jest --coverage --reporters=default",
    "dc": "yarn depcheck",
    "depcheck": "dependency-check . --unused --no-dev -i standard-log-color -i @mocktomata/io-client  && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --reporters=default",
    "test:browser": "karma start --single-run",
    "test:watch": "jest --watch",
    "test:watch:browser": "karma start",
    "verify": "yarn lint && yarn build && yarn dc && yarn coverage",
    "watch": "yarn test:watch",
    "watch:browser": "yarn test:watch:browser"
  },
  "dependencies": {
    "@mocktomata/file-server": "^7.0.0-beta.10",
    "@mocktomata/framework": "^7.0.0-beta.10",
    "@mocktomata/io-client": "^7.0.0-beta.10",
    "@mocktomata/io-local": "^7.0.0-beta.10",
    "async-fp": "^5.0.3",
    "chalk": "^4.1.1",
    "clibuilder": "^7.2.0",
    "global-store": "^1.0.0-beta.18",
    "standard-log": "^6.2.1",
    "standard-log-color": "^3.2.1",
    "type-plus": "^3.12.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@mocktomata/plugin-fixture-deep-link": "^1.0.12",
    "@mocktomata/plugin-fixture-dummy": "^1.1.10",
    "babel-loader": "^8.2.2",
    "core-js": "^3.14.0",
    "karma": "^6.3.3",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.1",
    "karma-typescript-es6-transform": "^5.5.1",
    "karma-webpack": "^5.0.0",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
