{
  "name": "@mocktomata/io-local",
  "version": "7.0.0-alpha",
  "description": "mocktomata local IO package",
  "homepage": "https://github.com/mocktomata/mocktomata",
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:es5 && yarn build:esnext",
    "build:es5": "tsc -p tsconfig.es5.json",
    "build:esnext": "tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib libm .ts",
    "coverage": "jest --coverage --reporters=default",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "eslint --ext=js,ts .",
    "test": "jest --reporters=default",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn build && yarn dc && yarn coverage",
    "watch": "yarn test:watch"
  },
  "dependencies": {
    "@mocktomata/framework": "^7.0.0-beta",
    "@mocktomata/io-fs": "^7.0.0-beta",
    "global-store": "^1.0.0-beta.11",
    "type-plus": "^1.29.0"
  },
  "devDependencies": {
    "@mocktomata/plugin-fixture-deep-link": "^1.0.2",
    "@mocktomata/plugin-fixture-dummy": "^1.0.3",
    "@mocktomata/plugin-fixture-no-activate": "^1.0.0",
    "@types/tmp": "^0.1.0",
    "assertron": "^7.1.2",
    "tmp": "^0.1.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
