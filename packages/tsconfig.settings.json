{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noUnusedLocals": true,
    "outDir": ".nowhere",
    "plugins": [
      {
        "name": "tslint-language-service"
      }
    ],
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2015"
  }
}
