{
  "name": "komondor-test",
  "description": "komondor plugin system",
  "version": "2.1.0",
  "main": "dist-es5/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist-es5"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es5",
    "build-es5": "tsc -p tsconfig.es5.json",
    "clean": "rimraf dist-es5",
    "codecov": "codecov",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "test": "tsc --noEmit && jest",
    "verify": "tsc --noEmit && npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "node scripts/watch.js",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata-test.git"
  },
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata-test/issues"
  },
  "homepage": "https://github.com/mocktomata/mocktomata-test",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": ".*\\.spec.ts$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "peerDependencies": {
    "komondor": ">=5.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.1",
    "assertron": "^5.0.0",
    "dependency-check": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-harmony": "^1.0.3",
    "jest": "^22.4.3",
    "komondor": "^6.0.1",
    "power-assert": "^1.5.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^19.0.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-unional": "^0.10.0",
    "typescript": "^2.8.1"
  }
}
