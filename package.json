{
  "name": "komondor",
  "description": "Your friendly guard dog for writing tests across boundaries",
  "version": "0.0.0-development",
  "main": "dist-es2015/index.js",
  "typings": "dist-es2015/index.d.ts",
  "files": [
    "dist-es2015"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es2015",
    "build-es5": "tsc -p tsconfig.es5.json",
    "build-es2015": "tsc -p tsconfig.es2015.json",
    "clean": "rimraf dist dist-es5 && rimraf dist-es2015",
    "codecov": "codecov",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "test": "npm run build && jest",
    "test-browser": "karma start karma.conf.js",
    "ci-verify": "npm run build && npm run lint && npm run coverage -- --maxWorkers=4 && npm run test-browser && npm run dependency-check",
    "verify": "npm run build && npm run lint && npm run coverage && npm run test-browser && npm run dependency-check",
    "watch": "npm run build && jest --watch",
    "semantic-release": "semantic-release"
  },
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/komondor.git"
  },
  "bugs": {
    "url": "https://github.com/unional/komondor/issues"
  },
  "homepage": "https://github.com/unional/komondor",
  "keywords": [
    "testing",
    "testing-tools",
    "testing-framework",
    "fixture",
    "boundary",
    "boundary-testing"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src2"
    ],
    "setupFiles": [
      "<rootDir>/scripts/setup-test-env.js"
    ],
    "testEnvironment": "node",
    "testRegex": "src2/.*.spec.(t|j)s$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.6",
    "@types/power-assert": "^1.5.0",
    "@types/ramda": "^0.25.36",
    "aurelia-logging-color": "^0.5.16",
    "aurelia-logging-memory": "^0.3.2",
    "cucumber": "^4.2.1",
    "delay": "^3.0.0",
    "dependency-check": "^3.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-harmony": "^1.0.4",
    "jest": "^23.4.2",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^3.0.13",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.5",
    "ts-jest": "^23.1.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-unional": "^0.9.6",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@unional/logging": "^0.5.3",
    "assertron": "^5.1.3",
    "chalk": "^2.4.1",
    "is-primitive": "^3.0.1",
    "komondor-plugin": "^1.7.7",
    "make-error": "^1.3.4",
    "ramda": "^0.25.0",
    "satisfier": "^4.5.0",
    "tersify": "^1.2.6",
    "unpartial": "^0.4.1"
  }
}
