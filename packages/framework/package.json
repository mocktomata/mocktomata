{
  "name": "@mocktomata/framework",
  "version": "7.0.0-beta.11",
  "description": "mocktomata framework package",
  "homepage": "https://github.com/mocktomata/mocktomata",
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts",
    "!ts/test-artifacts",
    "!**/*.mock.*",
    "!**/*.spec.*"
  ],
  "scripts": {
    "build": "run-p build:esm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rimraf coverage cjs esm lib libm tslib *.tsbuildinfo",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --reporters=default",
    "depcheck": "depcheck",
    "lint": "cross-env TIMING=1 eslint --ext=ts,js,cjs,mjs .",
    "nuke": "pnpm run clean && rimraf node_modules",
    "size": "size-limit",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --reporters=default",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "verify": "npm-run-all -p lint build coverage -p depcheck size",
    "watch": "pnpm run test:watch"
  },
  "dependencies": {
    "async-fp": "^8.0.2",
    "iso-error": "^4.4.0",
    "stack-utils": "^2.0.3",
    "standard-log": "^11.2.1",
    "tersify": "^3.10.5",
    "type-plus": "^4.14.1"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^8.0.1",
    "@types/stack-utils": "^2.0.1",
    "assertron": "^10.0.0",
    "delay": "^5.0.0",
    "depcheck": "^1.4.3",
    "dirname-filename-esm": "^1.1.1",
    "jest": "^29.2.1",
    "jest-validate": "29.2.1",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "satisfier": "^5.2.2",
    "size-limit": "^8.0.1",
    "standard-log-color": "^8.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "~4.8.3"
  },
  "engines": {
    "node": ">=14.16"
  }
}
