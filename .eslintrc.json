{
	"env": {
		"es6": true,
		"jest": true,
		"node": true
	},
	"extends": "plugin:harmony/latest",
	"overrides": [
		{
			"extends": "plugin:harmony/ts-prettier",
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"no-use-before-define": "off"
			}
		}
	],
	"parserOptions": {
		"sourceType": "module"
	},
	"root": true
}
