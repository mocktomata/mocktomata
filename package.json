{
  "name": "satisfier",
  "description": "Manage and generate artifacts to test data across boundaries",
  "version": "0.0.0-development",
  "main": "dist-es5/index.js",
  "module": "dist-es2015/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist",
    "dist-es5",
    "dist-es2015",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es5 && npm run build-es2015",
    "build-es5": "tsc -p tsconfig.es5.json",
    "build-es2015": "tsc -p tsconfig.es2015.json",
    "clean": "rimraf dist dist-es5 && rimraf dist-es2015",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "npm run clean && tsc && ava",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "npm run clean && node scripts/watch.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/assertron.git"
  },
  "bugs": {
    "url": "https://github.com/unional/assertron/issues"
  },
  "homepage": "https://github.com/unional/assertron",
  "ava": {
    "files": [
      "dist-es5/**/*.spec.js"
    ],
    "source": [
      "scripts/*.js",
      "dist-es5/**/*.js",
      "*.json"
    ]
  },
  "nyc": {
    "exclude": [
      "scripts",
      "**/*.spec.*",
      "**/fixtures/**/*"
    ]
  },
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "devDependencies": {
    "@types/node": "^8.0.47",
    "ava": "^0.24.0",
    "dependency-check": "^2.9.1",
    "eslint": "^4.10.0",
    "eslint-config-unional": "^0.2.3",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^11.0.2",
    "tslint": "^5.8.0",
    "tslint-config-unional": "^0.8.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "tersify": "^0.3.0"
  }
}
