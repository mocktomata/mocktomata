{
  "name": "@mocktomata/plugin-nodejs",
  "version": "0.1.10",
  "private": true,
  "description": "mocktomata plugin for nodejs",
  "keywords": [
    "mocktomata-plugin"
  ],
  "homepage": "https://github.com/mocktomata/mocktomata",
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "pnpm run build:lib && pnpm run build:libm",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:libm": "tsc -p tsconfig.libm.json",
    "clean": "rimraf lib libm .tsc",
    "dc": "pnpm run depcheck",
    "_depcheck": "dependency-check . --unused --no-dev -i '@mocktomata/framework' && dependency-check . --missing --no-dev",
    "lint": "cross-env TIMING=1 eslint --ext=ts,js,cjs,mjs .",
    "verify": "rimraf lib && tsc -p tsconfig.build.json && jest --coverage && pnpm run dependency-check"
  },
  "dependencies": {
    "@mocktomata/framework": "workspace:*"
  },
  "devDependencies": {
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "~4.7.4"
  },
  "engines": {
    "node": ">= 6"
  }
}
