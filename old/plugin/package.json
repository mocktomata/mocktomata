{
  "name": "@komondor/plugin",
  "version": "1.7.7",
  "description": "komondor plugin system",
  "homepage": "https://github.com/moctomata/moctomata",
  "bugs": {
    "url": "https://github.com/moctomata/moctomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moctomata/moctomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm run coverage && npm run dependency-check",
    "verify:ci": "npm run lint && npm run build && jest --coverage --maxWorkers=4 --config jest.config.ci.json && npm run dependency-check",
    "watch": "jest --watch"
  },
  "dependencies": {
    "make-error": "^1.3.5",
    "tersify": "^1.2.6"
  },
  "devDependencies": {
    "@types/jest": "^23.3.8",
    "@types/node": "^10.12.0",
    "assertron": "^5.2.2",
    "dependency-check": "^3.2.1",
    "eslint": "^5.8.0",
    "eslint-plugin-harmony": "^1.0.4",
    "jest": "^23.6.0",
    "jest-audio-reporter": "^2.2.0",
    "jest-junit": "^5.2.0",
    "jest-progress-tracker": "^2.0.1",
    "jest-watch-repeat": "^1.0.0",
    "jest-watch-suspend": "^1.1.0",
    "jest-watch-toggle-config": "^1.0.2",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-unional": "^0.10.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.1.3"
  }
}
