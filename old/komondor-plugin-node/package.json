{
  "name": "komondor-plugin-node",
  "description": "komondor plugin for node",
  "version": "1.3.0",
  "main": "dist-es5/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist-es5"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es5",
    "build-es5": "tsc -p tsconfig.es5.json",
    "clean": "rimraf dist-es5",
    "codecov": "codecov",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dependency-check": "dependency-check . --unused --no-dev -i komondor -i komondor-plugin && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "preversion": "git pull && npm run verify",
    "test": "jest",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "node scripts/watch.js",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata-plugin-node.git"
  },
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata-plugin-node/issues"
  },
  "homepage": "https://github.com/mocktomata/mocktomata-plugin-node",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/scripts/setup-test-env.js",
      "<rootDir>/scripts/test.ts"
    ],
    "testEnvironment": "node",
    "testRegex": ".*.(test|spec).(js|ts)$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "peerDependencies": {
    "komondor": ">=5",
    "komondor-plugin": ">=1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.6",
    "@unional/logging": "^0.5.3",
    "aurelia-logging-color": "^0.5.16",
    "dependency-check": "^3.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-harmony": "^1.0.4",
    "jest": "^23.4.2",
    "komondor": "^6.10.6",
    "komondor-plugin": "^1.7.7",
    "komondor-test": "^2.1.0",
    "node-fetch": "^2.2.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^19.0.0",
    "ts-jest": "^23.1.2",
    "tslint": "^5.11.0",
    "tslint-config-unional": "^0.9.6",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "satisfier": "^4.5.0"
  }
}
